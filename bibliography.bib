@article{bulutAgeSexInvariance2021,
  title = {Age and Sex Invariance of the {{Woodcock-Johnson IV}} Tests of Cognitive Abilities: {{Evidence}} from Psychometric Network Modeling},
  shorttitle = {Age and Sex Invariance of the Woodcock-Johnson {{IV}} Tests of Cognitive Abilities},
  author = {Bulut, Okan and Cormier, Damien C. and Aquilina, Alexandra M. and Bulut, Hatice C.},
  date = {2021-07-07},
  journaltitle = {Journal of Intelligence},
  shortjournal = {J. Intell.},
  volume = {9},
  number = {3},
  pages = {35},
  issn = {2079-3200},
  doi = {10.3390/jintelligence9030035},
  url = {https://www.mdpi.com/2079-3200/9/3/35},
  urldate = {2022-08-21},
  abstract = {The Woodcock-Johnson IV Tests of Cognitive Abilities (WJ IV COG) is a comprehensive assessment battery designed to assess broad and narrow cognitive abilities, as defined by the Cattell-Horn-Carroll (CHC) theory of intelligence. Previous studies examined the invariance of the WJ assessments across sex and age groups using factor analytic methods. Psychometric network modeling is an alternative methodology that can address both direct and indirect relationships among the observed variables. In this study, we employed psychometric network modeling to examine the invariance of the WJ IV COG across sex and age groups. Using a normative sample (n = 4212 participants) representative of the United States population, we tested the extent to which the factorial structure of the WJ IV COG aligned with CHC theory for the school-aged sample. Next, we used psychometric network modeling as a data-driven method to investigate whether the network structure of the WJ IV COG remains similar across different sex and age (age 6 to 19, inclusively) groups. Our results showed that the WJ IV COG maintained the same network structure across all age and sex groups, although the network structure at younger ages indicated weaker relationships among some subtests. Overall, the results provide construct validity evidence for the WJ IV COG, based on both theoretical and data-driven methods.},
  langid = {english}
}

