@book{cohenPsychologicalTestingAssessment2022,
  title = {Psychological Testing and Assessment: {{An}} Introduction to Tests and Measurement},
  shorttitle = {Psychological Testing and Assessment},
  author = {Cohen, Ronald Jay and Schneider, William Joel and Tobin, Renée Margaret},
  date = {2022},
  edition = {10},
  publisher = {McGraw Hill LLC},
  location = {New York},
  url = {https://www.mheducation.com/highered/product/1260837025.html},
  abstract = {"Psychological tests and other tools of assessment may differ with respect to a number of variables, such as content, format, administration procedures, scoring and interpretation procedures, and technical quality. The content (subject matter) of the test will, of course, vary with the focus of the particular test. But even two psychological tests purporting to measure the same thing-for example, personality-may differ widely in item content. This is so because two test developers might have entirely different views regarding what is important in measuring "personality"; different test developers employ different definitions of "personality." Additionally, different test developers come to the test development process with different theoretical orientations"--},
  isbn = {978-1-260-83702-5},
  keywords = {Psychological tests,Psychometrics,Textbooks}
}

