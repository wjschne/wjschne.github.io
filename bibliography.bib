@article{shuttleworth-edwardsCrossculturalEffectsIQ2004,
  title = {Cross-Cultural {{Effects}} on {{IQ Test Performance}}: {{A Review}} and {{Preliminary Normative Indications}} on {{WAIS-III Test Performance}}},
