@article{meyerScoresSpaceMultidimensional2018b,
  title = {Scores in {{Space}}: {{Multidimensional Scaling}} of the {{WISC-V}}},
  shorttitle = {Scores in {{Space}}},
  author = {Meyer, Emily M. and Reynolds, Matthew R.},
  year = {2018},
  month = sep,
  journal = {Journal of Psychoeducational Assessment},
  volume = {36},
  number = {6},
  pages = {562--575},
  issn = {0734-2829, 1557-5144},
  doi = {10.1177/0734282917696935},
  url = {http://journals.sagepub.com/doi/10.1177/0734282917696935},
  urldate = {2022-08-23},
  abstract = {The purpose of this study was to use multidimensional scaling (MDS) to investigate relations among scores from the standardization sample of the Wechsler Intelligence Scale for Children--Fifth edition (WISC-V; Wechsler, 2014). Nonmetric two-dimensional MDS maps were selected for interpretation. The most cognitively complex subtests and indexes were located near the center of the maps. Subtests were also grouped together in the way that they are organized in the primary and complementary indexes, and not necessarily on content surface features. Naming Speed and Symbol Translation scores may be better off kept as separate indexes, and Digit Span Sequencing appeared to add more complexity to the Digit Span subtest. One implication related to score interpretation is that General Ability Index (GAI) and Cognitive Proficiency Index (CPI) may be distinguished by the level of complexity of the subtests included in each index (i.e., more complex subtests included in GAI and less complex subtests included in the CPI).},
  langid = {english}
}

