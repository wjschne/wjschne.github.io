@incollection{woodcockFunctionalCattellHornCarrollNomenclature2018,
  title = {Functional {{Cattell-Horn-Carroll}} Nomenclature for Practical Applications},
  booktitle = {Contemporary Intellectual Assessment: {{Theories}}, Tests, and Issues},
  author = {Woodcock, Richard W. and Maricle, Denise E. and Miller, Daniel C. and McGill, Ryan J.},
  year = {2018},
  edition = {4},
  pages = {901--911},
  publisher = {Guilford Press},
  abstract = {This chapter reviews the evolution of Cattell--Horn--Carroll (CHC) theory, in an effort to examine the state of the state of the art in this dominant theory defining the structure of cognitive abilities. It suggests a more practical reconceptualization of CHC theory and functional nomenclature to describe CHC latent cognitive factors. The nomenclature associated with CHC theory has become more complex as scholars have elaborated upon the theory, introducing their additions and revisions to the taxonomy. In the late 1990s, Kevin McGrew negotiated the merger of Carroll's three-stratum theory with Cattell and Horn's fluid-crystallized model theory; he was thus instrumental in the ascendancy of the consolidated CHC model in the field of cognitive assessment. Philip Vernon's model was an important reconciliation of Charles Spearman's two-factor model and L. L. Thurstone's primary abilities. For any revision or refinement of the model to be considered valid, it too must be subjected to rigorous research, with each suggested addition, subtraction, or substitution carefully weighed and considered.}
}

