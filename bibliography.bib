@article{tucker-drobMethodsMeasuresConfirmatory2009,
  title = {Methods and Measures: {{Confirmatory}} Factor Analysis and Multidimensional Scaling for Construct Validation of Cognitive Abilities},
  shorttitle = {Methods and Measures},
  author = {Tucker-Drob, Elliot M. and Salthouse, Timothy A.},
