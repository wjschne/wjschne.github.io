@article{gignacFacePerceptionFacial2023,
  title = {Face Perception and Facial Emotional Expression Recognition Ability: {{Both}} Unique Predictors of the Broader Autism Phenotype},
  shorttitle = {Face Perception and Facial Emotional Expression Recognition Ability},
  author = {Gignac, Gilles E and Palermo, Romina and Bothe, Ellen and Walker, Dana L and Wilmer, Jeremy B},
  year = {2023},
