@article{gerstStructureProcessingSpeed2021,
  title = {The Structure of Processing Speed in Children and Its Impact on Reading},
  author = {Gerst, Elyssa H. and Cirino, Paul T. and Macdonald, Kelly T. and Miciak, Jeremy and Yoshida, Hanako and Woods, Steven P. and Gibbs, M. Cullen},
  year = {2021},
  month = jan,
  journal = {Journal of Cognition and Development},
  volume = {0},
