@incollection{breitSpearmanLawDiminishing2019,
  title = {Spearman's Law of Diminishing Returns and Its Implications for Theories of General Intelligence and Intelligence Testing},
  booktitle = {General and Specific Mental Abilities},
  author = {Breit, Moritz and Brunner, Martin and Preckel, Franzis},
  editor = {McFarland, Dennis J.},
  year = {2019},
  pages = {340--358},
  publisher = {Cambridge Scholars Publishing},
  address = {Newcastle upon Tyne, United Kingdom},
  abstract = {This chapter presents the phenomenon known as "Spearman's law of diminishing returns" (SLODR). SLODR belongs to the broader research field studying ability differentiation where variances of general and/or specific abilities vary with age or the level of general cognitive ability. Specifically, SLODR implies that the strength with which general intelligence (as represented by the general factor g) affects manifest test scores decreases for individuals with increasing levels of g, while the factor structure of intelligence is the same for all individuals across the whole ability range. Thus, in individuals with low cognitive ability, the g-factor is the main source of (reliable, observed) mental test variance. In individuals with high cognitive ability, however, its impact is reduced, increasing the relative importance of other, more specific cognitive abilities to explain mental test variance. The chapter discusses theoretical implications of this phenomenon for theories of general intelligence. It demonstrates practical implications regarding the ability dependent use and interpretation of intelligence tests by the results of two recent studies. (PsycInfo Database Record (c) 2021 APA, all rights reserved)},
  isbn = {978-1-5275-3310-3},
  keywords = {Cognitive Ability,Intelligence,Intelligence Measures,Theories},
  file = {C:\Users\renee\Zotero\storage\P6WZCB8C\2019-39185-013.html}
}

