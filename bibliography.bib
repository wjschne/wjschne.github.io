@article{feldsteinDoesSubtleScreening2007,
  title = {Does Subtle Screening for Substance Abuse Work? {{A}} Review of the {{Substance Abuse Subtle Screening Inventory}} ({{SASSI}})},
  shorttitle = {Does Subtle Screening for Substance Abuse Work?},
  author = {Feldstein, Sarah W. and Miller, William R.},
  date = {2007-01},
  journaltitle = {Addiction},
  shortjournal = {Addiction},
  volume = {102},
  number = {1},
  pages = {41--50},
  issn = {0965-2140, 1360-0443},
  doi = {10.1111/j.1360-0443.2006.01634.x},
  url = {https://onlinelibrary.wiley.com/doi/10.1111/j.1360-0443.2006.01634.x},
  urldate = {2024-06-03},
  abstract = {ABSTRACT                            Aim{$\quad$}               Through a complex combination of direct (face‐valid) and indirect (subtle) subscales, the Substance Abuse Subtle Screening Inventory (SASSI) is purported to detect substance use disorders with a high degree of validity regardless of respondent honesty or motivation. This review evaluates empirical evidence regarding the reliability and validity of this widely used screening instrument.                                         Methods{$\quad$}               Source documents were 36 peer‐reviewed reports yielding data regarding the SASSI's internal consistency, test–retest reliability, psychometric structure, convergent and divergent validity and criterion (predictive) validity.                                         Results{$\quad$}               The total               N               of the studies reviewed equaled 22{$\quad$}110. Internal consistency is high for the overall SASSI and for its direct but not its indirect (subtle) subscales, suggesting that the instrument taps a single face‐valid construct. SASSI classifications converged with those from other direct screening instruments, and were also correlated with ethnicity, general distress and social deviance. Studies found test–retest reliability lower than that reported in the test manuals. Sensitivity was found to be similar to that for public domain screening instruments, but on specificity the SASSI appears to yield a high rate of false positives.                                         Conclusion{$\quad$}               No empirical evidence was found for the SASSI's claimed unique advantage in detecting substance use disorders through its indirect (subtle) scales to circumvent respondent denial or dishonesty. Recommendations for screening and for future research with the SASSI are offered.},
  langid = {english}
}

