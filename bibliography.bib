@article{biesokAddingPiecePuzzle2024,
  title = {Adding a Piece to the Puzzle? {{The}} Allocation of Figurative Language Comprehension into the {{CHC}} Model of Cognitive Abilities},
  shorttitle = {Adding a Piece to the Puzzle?},
  author = {Biesok, Andra and Ziegler, Matthias and Montag, Christiane and Nenchev, Ivan},
  date = {2024-03-01},
  journaltitle = {Journal of Intelligence},
