@article{papadopoulosStructureSpeedProcessing2018,
  title = {The Structure of Speed of Processing across Cultures},
  author = {Papadopoulos, Timothy C. and Georgiou, George K. and Deng, Ciping and Das, J. P.},
  date = {2018-09-30},
  journaltitle = {Advances in Cognitive Psychology},
  shortjournal = {Adv Cogn Psychol},
  volume = {14},
  number = {3},
  eprint = {32337001},
  eprinttype = {pmid},
  pages = {112--125},
  issn = {1895-1171},
  doi = {10.5709/acp-0243-7},
  url = {https://www.ncbi.nlm.nih.gov/pmc/articles/PMC7171427/},
  urldate = {2021-03-08},
  abstract = {We examined whether a general processing factor emerges when using response times for cognitive processing tasks and whether such a factor is valid across three different cultural groups (Chinese, Canadian, and Greek). Three hundred twenty university students from Canada (n = 115), China (n = 110), and Cyprus (n = 95) were assessed on an adaptation of the Das-Naglieri Cognitive Assessment System (D-N CAS; ). Three alternative models were contrasted: a distinct abilities processing speed model (Model 1) that is dictated by the latent four cognitive factors of planning, attention, simultaneous and successive (PASS) processing, a unitary ability processing speed model (Model 2) that is dictated by the response time nature of all measures, and a bifactor model (Model 3) which included the latent scores of Models 1 and 2 and served as the full model. Results of structural equation modeling showed that (a) the model representing processing speed as a collection of four cognitive processes rather than a unitary processing speed factor was the most parsimonious, and (b) the loadings of the obtained factors were invariant across the three cultural groups. These findings enhance our understanding of the nature of speed of processing across diverse cultures and suggest that even when cognitive processes (i.e., PASS) are operationalized with response time measures, the processing component dominates speed.},
  pmcid = {PMC7171427},
  annotation = {ZSCC: 0000006}
}

