@article{nagyMaturationWhiteMatter2004,
  title = {Maturation of White Matter Is Associated with the Development of Cognitive Functions during Childhood},
  author = {Nagy, Zoltan and Westerberg, Helena and Klingberg, Torkel},
  year = {2004},
  month = sep,
  journal = {Journal of Cognitive Neuroscience},
  volume = {16},
  number = {7},
  pages = {1227--1233},
  issn = {0898-929X, 1530-8898},
  doi = {10.1162/0898929041920441},
  url = {https://direct.mit.edu/jocn/article/16/7/1227/3907/Maturation-of-White-Matter-is-Associated-with-the},
  urldate = {2022-08-23},
  abstract = {Abstract             In the human brain, myelination of axons continues until early adulthood and is thought to be important for the development of cognitive functions during childhood. We used diffusion tensor MR imaging and calculated fractional anisotropy, an indicator of myelination and axonal thickness, in children aged between 8 and 18 years. Development of working memory capacity was positively correlated with fractional anisotropy in two regions in the left frontal lobe, including a region between the superior frontal and parietal cortices.             Reading ability, on the other hand, was only correlated with fractional anisotropy in the left temporal lobe, in the same white matter region where adults with reading disability are known to have lower fractional anisotropy. Both the temporal and the frontal regions were also correlated with age. These results show that maturation of white matter is an important part of brain maturation during childhood, and that maturation of relatively restricted regions of white matter is correlated with development of specific cognitive functions.},
  langid = {english}
}

