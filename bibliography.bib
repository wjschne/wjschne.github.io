@article{towleEarlierBetterRelationship2020,
  title = {Is Earlier Better? {{The}} Relationship between Age When Starting Early Intervention and Outcomes for Children with Autism Spectrum Disorder: A Selective Review},
  shorttitle = {Is Earlier Better?},
  author = {Towle, Patricia O. and Patrick, Patricia A. and Ridgard, Tamique and Pham, Sofia and Marrus, Jaime},
  date = {2020-08-03},
  journaltitle = {Autism Research and Treatment},
  shortjournal = {Autism Research and Treatment},
  volume = {2020},
